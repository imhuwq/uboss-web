<%= content_for :page_title do %>  <%= @store.user.identify %><% end %><%= content_for :header_left do %>  <a class="back" onclick="history.go(-1)"><%= image_tag 'mobile/right.png' %></a><% end %><div class="dishes-list">  <% @dishes.each do |dishe| %>    <div class="dishes-box"       <% if 0 == 1 %>        <!--        没有图片的时候 （Ps：不要那种通用的默认图片）        -->        style="background-image:url('图片地址') ;"      <% end %>    >      <div class="dishes-info" data-product-id="<%= dishe.id %>">        <div class="title"><%= dishe.name %></div>        <br />        <div class="price"><small>￥</small><span class="dishes-price"><%= dishe.original_price %></span></div>        <div class="num-box">          <div class="minus remove-from-dishes disabled"><span>-</span></div>          <div class="num disabled">0</div>          <div class="plus pselect add-to-dishes"><span>+</span></div>        </div>      </div><!-- dishes-info -->    </div><!-- dishes-box-->  <% end %></div><!-- dishes-list --><!-- 商品规格 --><div class="dishes-pop-bg" id="product-specs"></div><!-- dishes-sku --><!-- end 商品规格 --><div class="dishes-buy-bar">  <div class="order-icon pull-left">    <span class="order-count" id="order-count"></span>  </div><!-- order-icon -->  <a class="order-buy-btn btn disabled pull-right" id="order-buy-btn">去付款</a><!-- order-buy-btn -->  <div class="dishes-order-price text-cut">    <small>应付：</small><span id="dishes-total-price">0</span> 元  </div><!-- order-info --></div><!-- dishes-buy-bar --><%= content_for :extra_javascript do %>  <%= javascript_include_tag "mobile_page/menus/boot" %><% end %>