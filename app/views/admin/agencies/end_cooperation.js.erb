if($('.list-table').length > 0){
  $("#cooperation_form_<%=@agency.id%>").closest('tr').remove()
  val = parseInt($('h1.num').text()) - 1
  $('h1.num').text(val)
  $('#agency-count').text(val)
}else{
  $("#cooperation_form_<%=@agency.id%>").html("<%= j render('admin/agencies/build_cooperation', agency: @agency) %>")
}
$(".modal-backdrop").remove()
$("body").removeClass('modal-open')
