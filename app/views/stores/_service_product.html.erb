<% cache ['store-product', service_product], expires_in: 1.day do %>
  <div class="box-w50 p-item" timestamp="<%= service_product.updated_at %>">
    <div class="product-box">
      <div class="cover">
        <%= link_to '', service_product_path(service_product), style: "background-image:url(#{service_product.image_url(:w320)})",class:'img',title:"#{service_product.name}" %>
        <%= image_tag("mobile/img.jpg") %>
      </div>
      <div class="title"><%= service_product.name %></div>
      <div class="price">ï¿¥<%= service_product.present_price %> </div>
    </div><!-- product-box -->
  </div><!-- box-w50 -->
<% end %>
