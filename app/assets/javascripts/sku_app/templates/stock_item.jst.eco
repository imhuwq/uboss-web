<% for pName of @sku_attributes: %>
  <% pValue = @sku_attributes[pName] %>
  <input type="hidden" name="product[product_inventories_attributes][<%= @id %>][sku_attributes][<%= pName %>]" value=<%= pValue %> >
  <td rowspan='1'><%= pValue %></td>
<% end %>
<td><input type='number' name="product[product_inventories_attributes][<%= @id %>][price]" value=<%= @price %> ></td>
<td><input type='number' name="product[product_inventories_attributes][<%= @id %>][count]" value=<%= @count %>></td>
