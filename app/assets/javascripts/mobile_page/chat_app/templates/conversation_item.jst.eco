<li targetType="<%= @targetType %>" targetId="<%= @targetId %>" targetName="<% @targetName %>">
  <span class="user_img">
    <img src='<%= @targetAvatar %>' onerror="this.src=<%= @errorAavatar %>" />
    <font class="conversation_msg_num #{hideMsgNum}"><%= @unreadMessageCount %></font>
  </span>
  <span class="conversationInfo">
    <font class="user_name"><%= @conversationTitle %></font>
    <font class="date" ><%= new Date(+@latestTime).toString().split(" ")[4] %></font>
  </span>
</li>
